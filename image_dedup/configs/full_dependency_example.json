{
  "image_loader": {
    "implementation": "standard",
    "parameters": {
      "max_dimension": 1024
    }
  },
  "perceptual_hash": {
    "implementation": "dct",
    "parameters": {
      "size": 16,
      "quality_factor": 0.9
    }
  },
  "storage": {
    "implementation": "local",
    "parameters": {}
  },
  "processing_config": {
    "implementation": "default",
    "parameters": {
      "max_concurrent": 8,
      "buffer_size": 200,
      "batch_size": 25,
      "enable_progress": true
    }
  },
  "progress_reporter": {
    "implementation": "console",
    "parameters": {
      "quiet": false
    }
  },
  "hash_persistence": {
    "implementation": "streaming_json",
    "parameters": {
      "buffer_size": 150
    }
  }
}