{
  "image_loader": {
    "implementation": "standard",
    "parameters": {
      "max_dimension": 2048
    }
  },
  "perceptual_hash": {
    "implementation": "dct",
    "parameters": {
      "size": 32,
      "quality_factor": 0.95
    }
  },
  "storage": {
    "implementation": "local",
    "parameters": {}
  },
  "processing_config": {
    "implementation": "default",
    "parameters": {
      "max_concurrent": 4,
      "buffer_size": 100,
      "batch_size": 10,
      "enable_progress": true
    }
  },
  "progress_reporter": {
    "implementation": "console",
    "parameters": {
      "quiet": false
    }
  },
  "hash_persistence": {
    "implementation": "streaming_json",
    "parameters": {
      "buffer_size": 50
    }
  }
}